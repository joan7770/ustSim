	lw	1	0	mcand
	lw	2	0	mplier
prod	lw	7	0	zero
mask	lw	6	0	one
	lw	5	0	zero	# i=0
loop	beq	5	2	after	# loop through mplier
	nand	4	2	6	# find if there is a 1 in the mplier using the mask
	beq	4	0	8
mult	add	3	1	4	# add the mcand and the bit of mplier
	sw	7	3	mult	# store prod into reg[7]
endlp	add	6	6	6	# shift left
	add	5	5	one	# i++	
	beq	0	0	loop
after	halt
mcand	.fill	29562
mplier	.fill	11834
zero	.fill	0
one	.fill	1
